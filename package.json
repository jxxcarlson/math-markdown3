{
  "name": "math-markdown3",
  "version": "1.0.0",
  "description": "pure elm markdown",
  "main": "public/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jxxcarlson/math-markdown3.git"
  },
  "author": "James Carlson <jxxcarlson@mac.com>",
  "license": "MIT",
  "devDependencies": {
    "@dillonkearns/elm-graphql": "^3.6.2",
    "elm": "^0.19.1-3",
    "elm-hot": "^1.0.1",
    "elm-test": "^0.19.1",
    "node-elm-compiler": "^5.0.4",
    "parcel-bundler": "^1.12.3"
  },
  "scripts": {
    "make": "elm make src/Main.elm --output=main.js",
    "publish": "git push",
    "build": "yarn run parcel build public/index.html",
    "start": "elm-live -d public/ src/Main.elm -- --output main.js",
    "test": "elm-test",
    "ff": "open -a /Applications/Firefox.app/ public/index.html",
    "chrome": "open -a /Applications/GoogleChrome.app/ public/index.html" ,
    "opera": "open -a /Applications/Opera.app/ public/index.html"
  },
